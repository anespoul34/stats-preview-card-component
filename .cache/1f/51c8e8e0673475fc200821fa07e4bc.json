 --font: 'Inter', sans-serif;\n  --font2: 'Lexend Deca', sans-serif;\n  --f-size: 15px;\n  --primary: hsl(244, 38%, 16%);\n  --secondary: hsl(277, 64%, 61%);\n  --white: hsl(0, 0%, 100%);\n  --light: hsl(225, 100%, 98%);\n  --medium: hsl(224, 23%, 55%);\n  --dark: hsl(223, 47%, 23%);\n  --black: hsl(0, 0%, 8%);\n  font-size: var(--f-size);\n}\n\n* {\n  box-sizing: border-box;\n  transition: all 0.2s ease-in-out;\n}\n\nhtml, body {\n  width: 100%;\n  height: 100vh;\n  margin: 0;\n  padding: 0;\n}\n\nbody {\n  display: flex;\n  flex-direction: column;\n  justify-content: flex-start;\n  align-items: center;\n  background-color: hsl(233, 47%, 7%);\n  padding: 20px;\n}\n\nh1, p, h2, a, span, button {\n  font-family: var(--font);\n  margin: 0;\n}\nh1 {\n  font-size: 2rem;\n  color: var(--white);\n  text-align: center;\n  margin-bottom: 10px;\n  font-weight: 700;\n}\nh1 span {\n  color: var(--secondary);\n}\nh2 {\n  font-size: 2rem;\n  color: var(--white);\n}\nh3 {\n  font-family: var(--font2);\n  font-weight: 400;\n  letter-spacing: 1px;\n  font-size: 0.9rem;\n  color: hsla(0, 0%, 100%, 0.6);\n  margin: 0;\n}\np {\n  font-size: 1rem;\n  line-height: 1.4;\n  color: hsla(0, 0%, 100%, 0.75);\n  text-align: center;\n  margin-bottom: 20px;\n}\n\nbutton {\n  cursor: pointer;\n  width: 100%;\n  height: 50px;\n  font-weight: 900;\n  border-radius: 10px;\n  text-align: center;\n  border: none;\n}\n\nmain {\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  align-items: center;\n  width: 100%;\n  height: auto;\n  border-radius: 10px;\n  background: var(--primary);\n}\n\nmain .header-img {\n  position: relative;\n  width: 100%;\n  height: 260px;\n  overflow: hidden;\n  border-radius: 10px 10px 0 0;\n}\n.header-img img {\n  width: 100%;\n}\n.header-img .purple-cover {\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  background:hsla(277, 100%, 41%, 0.5);\n}\n\nmain .main-container {\n  width: 100%;\n  padding: 40px 28px;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.datas-container {\n  margin-top: 15px;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  gap: 20px;\n}\n.data {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  gap: 5px;\n}\n\n.attribution {\n  width: 100%;\n  font-family: var(--font);\n  color: var(--white);\n  text-align: center;\n  font-size: 1rem;\n  margin-top: 10px;\n}\n.attribution a {\n  text-decoration: none;\n  font-weight: 700;\n  color: var(--secondary);\n}\n\n@media screen and (min-width: 768px) {\n  body {\n    justify-content: center;\n  }\n  main {\n    flex-direction: row-reverse;\n    width: 100%;\n    max-width: 1040px;\n    height: 100%;\n    max-height: 430px;\n    border-radius: 5px;\n  }\n  main .header-img {\n    width: 50%;\n    height: 100%;\n    display: flex;\n    justify-content: flex-end;\n    border-radius: 0 5px 5px 0;\n  }\n  .header-img img {\n    width: auto;\n    height: 100%;\n  }\n  main .main-container {\n    width: 50%;\n    height: 100%;\n    justify-content: space-between;\n  }\n  h1 {\n    margin-bottom: 30px;\n  }\n  main .main-container h1,\n  main .main-container p {\n    text-align: left;\n  }\n  main .main-container p {\n    line-height: 1.8;\n  }\n  .datas-container {\n    width: 100%;\n    flex: 1 0 auto;\n    flex-direction: row;\n    justify-content: space-between;\n    align-items: flex-end;\n  }\n  .data {\n    align-items: flex-start;\n  }\n  .attribution {\n    position: absolute;\n    bottom: 5px;\n    left: 0;\n  }\n}\n\n@media screen and (min-width: 900px) {\n  main .main-container {\n    padding: 60px 100px 60px 60px;\n  }\n}"},"lineCount":210}},"error":null,"hash":"e17657d104e030623a619d20eef6826c","cacheData":{"env":{}}}